{
    "languages": {
        "python": {
            "extension": "py",
            "comment" : {
                "top": "###",
                "prefix" : "# ",
                "bottom": "###"
            }
        },
        "haskell": {
            "extension": "hs",
            "comment" : {
                "top": "----",
                "prefix" : "-- ",
                "bottom": "----"
            }
        },
        "c": {
            "extension": "c",
            "comment" : {
                "top": "/*",
                "prefix" : "* ",
                "bottom": "*/"
            }
        }
    },
    "url_format": "https://projecteuler.net/problem={index}",
    "line_wrap": 89,
    "problem_padding": 2
}
